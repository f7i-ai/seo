{
	"name": "@prismicio/migrate",
	"version": "0.0.2",
	"description": "Set of helpers to migrate your data to Prismic",
	"keywords": [
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-migrate.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist",
		"./src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"prepare": "npm run build",
		"release": "npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
		"release:dry": "standard-version --dry-run",
		"release:alpha": "npm run test && standard-version --release-as major --prerelease alpha && git push --follow-tags && npm run build && npm publish --tag alpha",
		"release:alpha:dry": "standard-version --release-as major --prerelease alpha --dry-run",
		"lint": "eslint --ext .js,.ts .",
		"types": "vitest --typecheck.only --run",
		"types:watch": "vitest --typecheck.only",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"size": "size-limit",
		"test": "npm run lint && npm run types && npm run unit && npm run build && npm run size"
	},
	"dependencies": {
		"hast": "^1.0.0",
		"hast-util-select": "^6.0.2",
		"hast-util-to-html": "^9.0.3",
		"hast-util-to-string": "^3.0.0",
		"rehype-minify-whitespace": "^6.0.1",
		"rehype-parse": "^9.0.0",
		"unified": "^11.0.5",
		"unist-util-remove": "^4.0.0",
		"unist-util-visit": "^5.0.0",
		"vfile": "^6.0.3"
	},
	"devDependencies": {
		"@prismicio/client": "^7.10.1",
		"@size-limit/preset-small-lib": "^11.1.5",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/node": "^22.6.1",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitest/coverage-v8": "^2.1.1",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-tsdoc": "^0.3.0",
		"prettier": "^3.3.3",
		"prettier-plugin-jsdoc": "^1.3.0",
		"size-limit": "^11.1.5",
		"standard-version": "^9.5.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.7",
		"vite-plugin-sdk": "^0.1.2",
		"vitest": "^2.1.1"
	},
	"peerDependencies": {
		"@prismicio/client": "^7"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
